generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  username String @unique
  email    String @unique
  password String
}

model shortenUrl {
  id   String @id @default(auto()) @map("_id") @db.ObjectId
  url String
  slug String @unique
  clickCounter Int
}
